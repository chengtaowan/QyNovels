<?xml version="1.0" encoding="UTF-8"?>

    <manifest
    package="com.glong.sample"

    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application

        android:icon="@mipmap/iconz"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:theme="@style/AppTheme.Main"
        android:supportsRtl="true"
        android:allowBackup="true">


    -<activity android:name="com.glong.reader.MainActivity">


    -<intent-filter>

    <action android:name="android.intent.action.MAIN"/>

    <category android:name="android.intent.category.LAUNCHER"/>

</intent-filter>

</activity>

    <activity android:name="com.glong.reader.activities.SimpleReaderActivity" android:theme="@style/NoActionTheme"/>

    <activity android:name="com.glong.reader.activities.NormalReaderActivity" android:theme="@style/NoActionTheme"/>

    <activity android:name="com.glong.reader.activities.ExtendReaderActivity" android:theme="@style/NoActionTheme"/>

    <activity android:name="com.glong.reader.activities.CustomReaderActivity"/>

    <activity android:name="com.glong.reader.activities.WebViewActivity"/>

    <activity android:name="com.glong.reader.activities.PaperActivity"/>

</application>

</manifest>